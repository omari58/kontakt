{
  "realm": "kontakt",
  "clients": [
    {
      "clientId": "kontakt",
      "name": "Kontakt Backend",
      "enabled": true,
      "clientAuthenticatorType": "client-secret",
      "secret": "kontakt-local-secret",
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": false,
      "serviceAccountsEnabled": false,
      "publicClient": false,
      "attributes": {
        "pkce.code.challenge.method": "S256",
        "post.logout.redirect.uris": "http://localhost:5173##http://localhost"
      },
      "redirectUris": [
        "http://localhost:4000/api/auth/callback",
        "http://localhost/api/auth/callback"
      ],
      "webOrigins": [
        "http://localhost:4000",
        "http://localhost:5173",
        "http://localhost"
      ],
      "protocolMappers": [
        {
          "name": "realm-roles",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-realm-role-mapper",
          "config": {
            "multivalued": "true",
            "claim.name": "realm_access.roles",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "userinfo.token.claim": "true"
          }
        }
      ]
    }
  ],
  "users": [
    {
      "username": "admin@kontakt.local",
      "email": "admin@kontakt.local",
      "emailVerified": true,
      "enabled": true,
      "firstName": "Admin",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "admin",
          "temporary": false
        }
      ],
      "realmRoles": ["kontakt-admin"]
    },
    {
      "username": "user@kontakt.local",
      "email": "user@kontakt.local",
      "emailVerified": true,
      "enabled": true,
      "firstName": "Regular",
      "lastName": "User",
      "credentials": [
        {
          "type": "password",
          "value": "user",
          "temporary": false
        }
      ]
    }
  ]
}
